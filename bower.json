{
  "name": "upcycle",
  "version": "1.5.0",
  "homepage": "http://adsk-ui.github.io/upcycle/",
  "authors": [
    "admangum <adam.mangum@autodesk.com>"
  ],
  "description": "A home for our efforts to transition one-offs and semi-vetted code into a library of high quality, well tested, reusable components",
  "main": [
    "build/upcycle.js",
    "build/themes/base/base.css",
    "build/themes/portal/portal.css"
  ],
  "keywords": [
    "js",
    "components"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "src"
  ],
  "dependencies": {
    "jquery": "1.8.3",
    "jquery-ui": "1.10.4",
    "handlebars": "^4.0.5",
    "underscore": "1.6.0",
    "tinyscrollbar": "https://github.com/adsk-ui/tinyscrollbar.git#2.1.9",
    "jquery-i18n-properties": "https://github.com/adsk-ui/jquery-i18n-properties.git#1.0.0",
    "bootstrap": "2.3.2"
  },
  "devDependencies": {
    "mocha": "~1.18.2",
    "chai": "~1.9.1",
    "sinon": "~1.10.3"
  },
  "overrides": {
    "jquery-ui": {
      "main": [
        "ui/jquery.ui.core.js",
        "ui/jquery.ui.widget.js"
      ]
    },
    "tinyscrollbar": {
      "main": "lib/jquery.tinyscrollbar.js"
    },
    "mocha": {
      "main": [
        "mocha.css",
        "mocha.js"
      ]
    },
    "tablesorter": {
      "main": "jquery.tablesorter.js"
    },
    "sinon": {
      "main": [
        "lib/sinon.js",
        "lib/sinon/spy.js",
        "lib/sinon/call.js"
      ]
    }
  },
  "resolutions": {
    "jquery": "1.8.3"
  }
}
